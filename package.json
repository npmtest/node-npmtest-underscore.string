{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "description": "test coverage for [underscore.string (v3.3.4)](http://epeli.github.com/underscore.string/) [![npm package](https://img.shields.io/npm/v/npmtest-underscore.string.svg?style=flat-square)](https://www.npmjs.org/package/npmtest-underscore.string) [![travis-ci.org build-status](https://api.travis-ci.org/npmtest/node-npmtest-underscore.string.svg)](https://travis-ci.org/npmtest/node-npmtest-underscore.string)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "underscore.string": "*",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmtest/node-npmtest-underscore.string",
    "keywords": [
        "coverage",
        "test",
        "underscore.string"
    ],
    "license": "MIT",
    "main": "lib.npmtest_underscore.string.js",
    "name": "npmtest-underscore.string",
    "nameAlias": "npmtest_underscore_string",
    "nameOriginal": "npmtest-underscore.string",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmtest/node-npmtest-underscore.string.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "0.0.2"
